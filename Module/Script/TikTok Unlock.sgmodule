#!name=TikTok
#!desc=解除 TikTok 区域限制。
#!system=ios

[URL Rewrite]
(?<=_region=)CN(?=&) url 307 TW
(?<=&app_version=)1[6-9]..(?=.?.?&) url 307 17..
(?<=\?version_code=)1[6-9]..(?=.?.?&) url 307 17..
^https://api[\w-]*?.tiktokv.com/aweme/v\d/policy/notice/? url reject

[MITM]
hostname = %INSERT% *.tiktokv.com, *.byteoversea.com, *.musical.ly, *.snssdk.com